class Insan:
    def __init__(self, yas):
        self.yas = yas

    def kateqoriya(self):
        if self.yas < 0:
            return "yanlis yas"
        elif self.yas <= 12:
            return "usaq"
        elif self.yas <= 17:
            return "yeniyetme"
        elif self.yas <= 60:
            return "boyuk"
        else:
            return "qoca"
yas = int(input("Yasinizi daxil edin: "))
insan = Insan(yas)
print(insan.kateqoriya())